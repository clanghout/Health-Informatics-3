<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<Tab text="Data" fx:controller="controllers.DataController" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<AnchorPane fx:id="root">
			<children>
				<TextField
						fx:id="fileNameField"
						styleClass="file-name-field"
						layoutX="14.0"
						layoutY="27.0"
						disable="true" />
				<Button
						styleClass="import-button"
						layoutX="476.0"
						layoutY="27.0"
						onAction="#handleImportButtonAction"
						text="Import" />
				<Label layoutX="14.0" layoutY="7.0" text="Import data" />
				<Button
					styleClass="analyse-button"
					layoutX="476.0"
					layoutY="60.0"
					onAction="#handleAnalyseButtonAction"
					text="Read" />
				<Button
						styleClass="save-button"
						layoutX="476.0"
						layoutY="93.0"
						onAction="#handleSaveButtonAction"
						text="Save" />
			</children>
		</AnchorPane>
	</content>
</Tab>
