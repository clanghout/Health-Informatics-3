<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Tab text="Data" fx:controller="ui.DataController" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<AnchorPane fx:id="root">
			<children>
				<TextField fx:id="fileNameField" styleClass="file-name-field" layoutX="14.0" layoutY="27.0" />
				<Button
						styleClass="import-button"
						layoutX="476.0"
						layoutY="27.0"
						onAction="#handleImportButtonAction"
						text="Import" />
				<Label layoutX="14.0" layoutY="7.0" text="Import data" />
			</children>
		</AnchorPane>
	</content>
</Tab>
