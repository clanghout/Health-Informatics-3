<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>
<Tab
		text="Analysis"
		fx:controller="ui.AnalysisController"
		xmlns="http://javafx.com/javafx/8"
		xmlns:fx="http://javafx.com/fxml/1"
		>
	<content>
		<AnchorPane>
			<children>
				<VBox styleClass="analysis-buttons" layoutX="14.0" layoutY="14.0">
					<children>
						<Button text="Load" />
						<Button text="Save" />
						<Button text="Execute" onAction="#handleExecuteButtonAction" />
					</children>
				</VBox>
				<TextFlow layoutX="114.0" layoutY="14.0">
					<children>
						<TextArea styleClass="analysis-text" fx:id="userscript" />
					</children>
				</TextFlow>
			</children>
		</AnchorPane>
	</content>
</Tab>
