<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Tab fx:controller="ui.TableViewController" text="View" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<content>
	<AnchorPane>
		 <children>
			<Button layoutX="562.0" layoutY="263.0" text="Save" />
				<ListView styleClass="model-list" fx:id="inputtables" layoutX="14.0" layoutY="18.0" />
				<SplitPane styleClass="data-container" dividerPositions="0.5" layoutX="147.0" layoutY="18.0">
					<items>
						<AnchorPane styleClass="table-container">
							<children>
								<TableView styleClass="data-table" 
								fx:id="tableView"/>
							</children>
						</AnchorPane>
						<AnchorPane styleClass="data-fields-container" fx:id="datafields" />
					</items>
				</SplitPane>
				<Button
                    styleClass="analyse-button"
                    onAction="#fillTable"
                    text="Analyze" />
		 </children>
	</AnchorPane>
</content>
</Tab>
