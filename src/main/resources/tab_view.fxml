<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<Tab fx:controller="controllers.TableViewController" text="View" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <VBox>
            <children>
                <SplitPane styleClass="vertical-split" dividerPositions="0.8">
                    <items>
                        <SplitPane styleClass="horizontal-split" dividerPositions="0.2" >
                            <items>
                                <ListView fx:id="inputTables" />
                                <TableView fx:id="tableView" />
                            </items>
                        </SplitPane>
                    </items>
                </SplitPane>
            </children>
        </VBox>
    </content>
</Tab>