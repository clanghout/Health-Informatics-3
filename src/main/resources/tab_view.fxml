<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<Tab fx:controller="controllers.TableViewController" text="View" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <VBox>
            <children>
                <SplitPane styleClass="horizontal-split" dividerPositions="0.2" >
                    <items>
                        <ListView fx:id="inputTables" />
                        <TableView fx:id="tableView" />
                    </items>
                </SplitPane>
            </children>
        </VBox>
    </content>
</Tab>